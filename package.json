{
  "name": "find-up-json",
  "repository": "github:fabiospampinato/find-up-json",
  "description": "Find, read and parse the first matching file found walking the filesystem upwards.",
  "version": "2.0.1",
  "type": "module",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "tsex clean",
    "compile": "tsex compile",
